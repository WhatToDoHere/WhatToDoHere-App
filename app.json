{
  "expo": {
    "name": "WhatToDoHere",
    "slug": "WhatToDoHere",
    "scheme": "WhatToDoHere",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "jsEngine": "hermes",
    "experiments": {
      "turboModules": true
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.moonstrnck.WhatToDoHere",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to provide better services.",
        "NSLocationAlwaysUsageDescription": "We need your location to provide better services."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.moonstrnck.WhatToDoHere"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1d4d505d-bb55-4b8d-b007-289a9140f265"
      }
    },
    "owner": "moonstrnck",
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow WhatToDoHere to use your location.",
          "isIosBackgroundLocationEnabled": true
        }
      ],
      [
        "expo-font",
        {
          "font": "./assets/fonts/Opposit-Bold.ttf"
        }
      ],
      [
        "expo-font",
        {
          "font": "./assets/fonts/Opposit-Light.ttf"
        }
      ],
      [
        "expo-font",
        {
          "font": "./assets/fonts/Opposit-Medium.ttf"
        }
      ],
      [
        "expo-font",
        {
          "font": "./assets/fonts/Opposit-Regular.ttf"
        }
      ],
      [
        "expo-font",
        {
          "font": "./assets/fonts/Pretendard-Bold.otf"
        }
      ],
      [
        "expo-font",
        {
          "font": "./assets/fonts/Pretendard-Light.otf"
        }
      ],
      [
        "expo-font",
        {
          "font": "./assets/fonts/Pretendard-Medium.otf"
        }
      ],
      [
        "expo-font",
        {
          "font": "./assets/fonts/Pretendard-Regular.otf"
        }
      ]
    ]
  }
}
