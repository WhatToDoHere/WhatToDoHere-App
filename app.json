{
  "expo": {
    "name": "WhatToDoHere",
    "slug": "WhatToDoHere",
    "scheme": "WhatToDoHere",
    "version": "1.0.2",
    "orInterfaceStyle": "light",
    "splash": {
      "imrientation": "portrait",
      "icon": "./assets/icon.png",
      "useage": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "jsEngine": "hermes",
    "experiments": {
      "turboModules": true
    },
    "ios": {
      "entitlements": {
        "com.apple.developer.networking.wifi-info": true
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.moonstrnck.WhatToDoHere",
      "buildNumber": "3",
      "infoPlist": {
        "UIBackgroundModes": ["location", "fetch", "remote-notification"],
        "NSLocationWhenInUseUsageDescription": "We need your location to provide better services.",
        "NSLocationAlwaysUsageDescription": "We need your location to provide better services."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.moonstrnck.WhatToDoHere"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1d4d505d-bb55-4b8d-b007-289a9140f265"
      }
    },
    "owner": "moonstrnck",
    "plugins": [
      "expo-router",
      "react-native-wifi-reborn",
      [
        "expo-image-picker",
        {
          "photosPermission": "WhatToDoHere가 사용자의 사진에 접근하도록 허용합니다.",
          "cameraPermission": "WhatToDoHere가 카메라를 사용하도록 허용합니다."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "WhatToDoHere가 사용자의 위치 정보를 사용하도록 허용합니다.",
          "isIosBackgroundLocationEnabled": true
        }
      ],
      [
        "expo-font",
        {
          "font": "./assets/fonts/Opposit-Bold.ttf"
        }
      ],
      [
        "expo-font",
        {
          "font": "./assets/fonts/Opposit-Light.ttf"
        }
      ],
      [
        "expo-font",
        {
          "font": "./assets/fonts/Opposit-Medium.ttf"
        }
      ],
      [
        "expo-font",
        {
          "font": "./assets/fonts/Opposit-Regular.ttf"
        }
      ],
      [
        "expo-font",
        {
          "font": "./assets/fonts/Pretendard-Bold.otf"
        }
      ],
      [
        "expo-font",
        {
          "font": "./assets/fonts/Pretendard-Light.otf"
        }
      ],
      [
        "expo-font",
        {
          "font": "./assets/fonts/Pretendard-Medium.otf"
        }
      ],
      [
        "expo-font",
        {
          "font": "./assets/fonts/Pretendard-Regular.otf"
        }
      ]
    ]
  }
}
